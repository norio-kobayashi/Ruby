<div class="container-fluid" id="landing-profiles-container">
  <!-- THE HEADER -->
  <div class="row-fluid">
    <div class="span12">
      <%= image_tag "logo-header.png", border: "0", vspace: "20px" %>
    </div>
    <div class="span12">
      <h2 class="text-info"><center>Sign up to HyeDating.com to get access<br/>to Armenian Singles looking to meet.</center></h2>
    </div>
  </div>
  <div class="row-fluid">
    <div class="span12" id="main">
      <div class="row-fluid">
        <% @users.each do |user| %>
          <%= render 'seo/user', user: user.decorate %>
        <% end %>
        <%= paginate @users %>
      </div>
    </div>
  </div>
  <style type="text/css" media="screen">
    #i_100 {
      height: 100px;
      width: 100px;
    }
  </style>
